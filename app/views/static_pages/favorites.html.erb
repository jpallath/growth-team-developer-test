<div class="favoritez">
  <h2>Your Favorite Gems</h2>
<div class="favoritesContainer">

</div>
</div>
<script type="text/javascript">
  for (var i = 0; i < localStorage.length; i++) {
    var gemmy = localStorage.getItem(localStorage.key(i))
    var htmlGem = "<p>"+gemmy+"</p>"
    var anchoredGem = "<a href=https://rubygems.org/gems/"+gemmy+">"+htmlGem+"</a>"
    var stars = "<div class='star' id="+gemmy+"></div>"+anchoredGem;
    console.log(stars);
   $(".favoritesContainer").append("<div class='favorited'>"+stars+"</div>")
  }
</script>
